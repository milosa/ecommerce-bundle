{
  "name": "milosa/ecommerce-symfony",
  "description": "description_text",
  "minimum-stability": "dev",
  "license": "MIT",
  "version": "0.0.1",
  "prefer-stable": true,
  "authors": [
    {
      "name": "milosa"
    }
  ],

  "repositories": [
    {
      "type": "path",
      "url": "../ecommerce",
      "options": {
        "symlink": true
      }
    }
  ],

  "require-dev": {
    "phpunit/phpunit": "^5.5.4",
    "symfony/browser-kit" : "^3.1.5",
    "symfony/twig-bundle" : "^3.1.5"
  },
  "require": {
    "php": "^7.0",
    "symfony/framework-bundle": "^3.1",
    "milosa/ecommerce": ">=0.0.1"
  },

  "autoload": {
    "psr-4": {
      "Milosa\\EcommerceBundle\\": "src/"
    }
  },



  "autoload-dev": {
    "psr-4": {
      "Milosa\\EcommerceBundle\\Tests\\": "tests/"
    }
  }
}
